{
  "name": "@mdornseif/fastify-for-appengine",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Maximillian Dornseif",
  "main": "dist/index.js",
  "module": "dist/fastify-for-appengine.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "vitest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/fastify-for-appengine.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fastify-for-appengine.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.0",
    "@tsconfig/recommended": "^1.0.3",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "size-limit": "^11.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "nanoid": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdornseif/fastify-for-appengine.git"
  }
}
